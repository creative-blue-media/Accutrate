w class=\"mb-0\">\r\n              <b-colxx xxs=\"12\">\r\n                <b-form-group>\r\n                  <v-date-picker\r\n                    mode=\"single\"\r\n                    class=\"is-expanded\"\r\n                    v-model=\"selectedValueSingleInline\"\r\n                    :is-inline=\"true\"\r\n                    :input-props=\"{ class:'form-control', placeholder: $t('form-components.date') }\"\r\n                  ></v-date-picker>\r\n                </b-form-group>\r\n              </b-colxx>\r\n            </b-row>\r\n          </b-form>\r\n        </b-card>\r\n      </b-colxx>\r\n    </b-row>\r\n\r\n    <b-row>\r\n      <b-colxx xxs=\"12\">\r\n        <b-card class=\"mb-4\" :title=\"$t('form-components.dropzone')\">\r\n          <b-form>\r\n            <b-row>\r\n              <b-colxx xxs=\"12\">\r\n                <vue-dropzone ref=\"myVueDropzone\" id=\"dropzone\" :options=\"dropzoneOptions\"></vue-dropzone>\r\n              </b-colxx>\r\n            </b-row>\r\n          </b-form>\r\n        </b-card>\r\n      </b-colxx>\r\n    </b-row>\r\n\r\n    <b-row>\r\n      <b-colxx xxs=\"12\">\r\n        <b-card class=\"mb-4\" :title=\"$t('form-components.tags')\">\r\n          <b-form>\r\n            <b-row>\r\n              <b-colxx xxs=\"12\">\r\n                <input-tag v-model=\"tags\" :placeholder=\"$t('form-components.tags')\"></input-tag>\r\n              </b-colxx>\r\n            </b-row>\r\n          </b-form>\r\n        </b-card>\r\n      </b-colxx>\r\n    </b-row>\r\n\r\n    <b-row>\r\n      <b-colxx xxs=\"12\">\r\n        <b-card class=\"mb-4\" :title=\"$t('form-components.switch')\">\r\n          <b-form>\r\n            <b-row class=\"mb-3\">\r\n              <b-colxx xxs=\"6\">\r\n                <b-row>\r\n                  <b-colxx xxs=\"12\">\r\n                    <label>{{$t('form-components.primary')}}</label>\r\n                  </b-colxx>\r\n                  <b-colxx xxs=\"12\">\r\n                    <switches v-model